{
  "editor.detectIndentation": false,
  "editor.tabSize": 4,
  "rust-analyzer.procMacro.enable": false,
  "cSpell.words": ["despawn", "Numpad", "spawnable", "tdlg"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "target": true,
    "build": true,
    "*.zip": true
  },
  "[plaintext]": {
    "editor.quickSuggestions": false
  }
}
